@model DRK.ProgDec.BL.Models.ShoppingCart

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Items[0].ChangeDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Items[0].StudentName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Items[0].ProgramName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Items[0].ImagePath)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Items[0].DegreeTypeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Items) {
        <tr>
          
            <td>
                @Html.DisplayFor(modelItem => item.ChangeDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgramName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagePath)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DegreeTypeName)
            </td>
            <td>
                @Html.ActionLink("Remove", "Remove", new {  id=item.ID })
            </td>
        </tr>
}
    </tbody>
</table>
